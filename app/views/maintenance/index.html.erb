<p id="notice"><%= notice %></p>

<h1>Listing Maintenance Requests</h1>

<table>
  <thead>
    <tr>
      <th>Problem</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maintenance.each do |maintenance| %>
      <tr>
		<td><%= maintenance.id %></td>
        <td><%= maintenance.issue %></td>
        <td><%= maintenance.created_at %></td>
		    <td><%= user.fixed %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request', new_maintenance_path %>
